{
  "unstable": [
    "kv"
  ],
  "tasks": {
    "build": "deno run --allow-all scripts/build.ts",
    "cache": "deno run --allow-all scripts/cache.ts",
    "cache:clean": "rm -rf cache",
    "gen": "deno run --allow-net --allow-read --allow-write=./routes.ts scripts/gen.ts",
    "start": "deno run --allow-net --allow-read --allow-env --env --allow-sys --allow-run --allow-write=./routes.ts,.store --allow-hrtime --watch scripts/dev.ts",
    "start:prod": "deno run --allow-net --allow-env main.ts",
    "mkcert": "mkcert -install -key-file localhost-key.pem -cert-file localhost-cert.pem localhost",
    "deploy": "deno run --allow-net --allow-read --allow-write --allow-env https://deno.land/x/deploy@1.7.0/deployctl.ts deploy --import-map=deno.json --project=jollytoad --exclude=.env,localhost-cert.pem,localhost-key.pem,scripts main.ts",
    "check": "deno fmt && deno lint && deno check **/*.ts",
    "lock": "rm -f deno.lock && deno check **/*.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "$jsx",
    "verbatimModuleSyntax": true
  },
  "exclude": [
    "static/sw.js",
    "static/sw_compat.js",
    "static/prism.*"
  ],
  "fmt": {
    "exclude": ["routes.ts"]
  },
  "imports": {
    "@/": "./",
    "$std/": "https://deno.land/std@0.218.0/",
    "https://deno.land/std@0.215.0/": "https://deno.land/std@0.218.0/",
    "https://deno.land/std@0.213.0/": "https://deno.land/std@0.218.0/",
    "https://deno.land/std@0.208.0/": "https://deno.land/std@0.218.0/",
    "$http_fns/": "https://deno.land/x/http_fns@v0.5.0/lib/",
    "$http_render_fns/": "https://deno.land/x/http_render_fns@v0.1.0/lib/",
    "$jsx/": "https://deno.land/x/jsx_stream@v0.0.13/",
    "$jsx/jsx-runtime": "https://deno.land/x/jsx_stream@v0.0.13/jsx-runtime.ts",
    "$deno_kv_oauth/": "https://deno.land/x/deno_kv_oauth@v0.10.0/lib/",
    "https://deno.land/x/oauth2_client@v1.0.2/mod.ts": "https://cdn.jsdelivr.net/gh/jollytoad/deno-oauth2-client@oidc-2/mod.ts",
    "$store": "https://deno.land/x/storage_modules@v0.1.0/deno_kv.ts",
    "esbuild": "https://deno.land/x/esbuild@v0.20.1/mod.js",
    "esbuild_deno_loader": "https://deno.land/x/esbuild_deno_loader@0.9.0/mod.ts",
    "https://deno.land/x/esbuild@v0.20.0/": "https://deno.land/x/esbuild@v0.20.1/"
  }
}
