{
  "tasks": {
    "gen": "deno run --config=deno.json --allow-net --allow-read=./routes,./routes.ts --allow-write=./routes.ts https://deno.land/x/ahx_fns@v0.0.10/scripts/gen.ts",
    "start": "deno run --unstable --allow-net --allow-read --allow-env --allow-sys --watch dev.ts",
    "start:prod": "deno run --unstable --allow-net --allow-env main.ts",
    "mkcert": "mkcert -install -key-file localhost-key.pem -cert-file localhost-cert.pem localhost",
    "deploy": "deno run --allow-net --allow-read --allow-write --allow-env https://deno.land/x/deploy@1.6.0/deployctl.ts deploy --import-map=deno.json --project=loud-fox-12 --exclude=.env,localhost-cert.pem,localhost-key.pem main.ts",
    "check": "deno fmt && deno lint && deno --unstable check **/*.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "$jsx"
  },
  "imports": {
    "@/": "./",
    "$std/": "https://deno.land/std@0.192.0/",
    "$http_fns/": "https://deno.land/x/http_fns@v0.0.15/",
    "$http_render_fns/": "https://deno.land/x/http_render_fns@v0.0.4/",
    "$ahx_fns/": "https://deno.land/x/ahx_fns@v0.0.10/",
    "$jsx/": "https://deno.land/x/jsx_stream@v0.0.7/",
    "$jsx/jsx-runtime": "https://deno.land/x/jsx_stream@v0.0.7/jsx-runtime.ts",
    "$deno_kv_oauth/": "https://deno.land/x/deno_kv_oauth@v0.2.5/src/"
  }
}
